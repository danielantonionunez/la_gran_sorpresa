<% f1 = egreso_presupuesto_mensual.ejecucion_egresos.build %>
<%= form_for([@presupuesto_mensual, egreso_presupuesto_mensual, f1]) do |f| %>
  <% if f1.errors.any? %>
    <div id="error_explanation">
      <h2>No se puede grabar porque no ha pasado la validacion <%= pluralize(f1.errors.count, "dato") %>:</h2>
      <ul>
      <% f1.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class="field">
    <%= f.label :forma_de_pago_al_contado_id, :class => :menuNormal1N %><br />
    <%= f.collection_select :forma_de_pago_al_contado_id,egreso_presupuesto_mensual.item.forma_de_pago_al_contados,:id,:nombre, {:include_blank => "Seleccione valor"}, {:class => :NormalSelect} %>
  </div>
  <div class="field">
    <%= f.label :forma_de_pago_al_credito_id, :class => :menuNormal1N %><br />
    <%= f.collection_select :forma_de_pago_al_credito_id,egreso_presupuesto_mensual.item.forma_de_pago_al_creditos,:id,:nombre, {:include_blank => "Seleccione valor"}, {:class => :NormalSelect} %>
  </div>
  <div class="field">
    <%= f.label :precio, :class => :menuNormal1N %><br />
    <%= f.text_field :precio, :class => :NormalCajaTexto %>
  </div>
  <div class="actions">
    <%= f.submit :class => :NormalBoton %>
  </div>
<% end %>