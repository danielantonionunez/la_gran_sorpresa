<%= form_for([@presupuesto_mensual, ingreso_presupuesto_mensual, ingreso_presupuesto_mensual.ejecucion_ingresos.build]) do |f| %>
  <div class="field">
    <%= f.label :forma_de_pago_al_contado_id, :class => :menuNormal1N %><br />
    <%= f.collection_select :forma_de_pago_al_contado_id,ingreso_presupuesto_mensual.item.forma_de_pago_al_contados,:id,:nombre, {:include_blank => "Seleccione valor"}, {:class => :NormalSelect} %>
  </div>
  <div class="field">
    <%= f.label :forma_de_pago_al_credito_id, :class => :menuNormal1N %><br />
    <%= f.collection_select :forma_de_pago_al_credito_id,ingreso_presupuesto_mensual.item.forma_de_pago_al_creditos,:id,:nombre, {:include_blank => "Seleccione valor"}, {:class => :NormalSelect} %>
  </div>
  <div class="field">
    <%= f.label :precio, :class => :menuNormal1N %><br />
    <%= f.text_field :precio, :class => :NormalCajaTexto %>
  </div>
  <div class="actions">
    <%= f.submit :class => :NormalBoton %>
  </div>
<% end %>